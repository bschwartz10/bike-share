<h1>New Station</h1>

<form class="well" action="/stations" method="post">
  <div class="form-group">
    <label>Name</label>
    <input class="form-control" type='text'  name='station[name]'>
  </div>
  <div class="form-group">
    <label>City</label>
    <input class="form-control" type='text'  name='station[city]' ><br>
  </div>
  <div class="form-group">
    <label>Dock Count</label>
    <input class="form-control" type='number'  name='station[dock_count]'><br>
  </div>
  <div class="form-group">
    <label>Installation Date</label>
    <input class="form-control" type='date'  name='station[installation_date]'><br>
  </div>
  <input type='submit' class="btn btn-primary" value='Submit'>
</form>
